{
  "env": {
    "commonjs": true,
    "browser": true,
    "es6": true,
    "node": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "rules": {
    "no-console": "error",
    "react/prop-types": 0
  },
  "settings": {
    "react": {
      "pragma": "React",
      "version": "16.12.0"
    }
  }
}
